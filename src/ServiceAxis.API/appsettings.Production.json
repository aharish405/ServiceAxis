{
  "AppSettings": {
    "Environment": "Production",
    "ShowDetailedErrors": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=tcp:YOUR_SQL_SERVER;Initial Catalog=ServiceAxis;User ID=sa;Password=YOUR_PASSWORD;Connect Timeout=30;Encrypt=True"
  },
  "JwtSettings": {
    "SecretKey": "REPLACE_WITH_ACTUAL_PRODUCTION_SECRET_MIN_32_CHARS"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "Hangfire": "Error"
      }
    }
  }
}
